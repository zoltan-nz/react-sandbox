    "test": "react-scripts-ts test --env=jsdom --coverage",
    "lint": "tslint --project tsconfig.json --config tslint.json --fix",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "del-cli": "^1.1.0",
    "typescript": "^2.9.2"
  }
}
