{
  "name": "react-sandbox",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "clean": "del build coverage",
    "start:dev": "react-scripts-ts start",
    "build": "npm run clean && CI=true react-scripts-ts build",
